{
  "name": "admin-login-fix",
  "version": "1.0.0",
  "description": "Scripts to fix admin login issues in Strike app",
  "main": "fix-all.js",
  "scripts": {
    "fix": "node fix-all.js",
    "fix-password": "node fix-admin-password.js",
